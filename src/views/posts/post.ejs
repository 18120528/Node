<%- include ("../_partial/_header.ejs"); %>
    <div class="row">
        <div class="col-md-6">
            <h1>Post Page - VNUHCM</h1>
        </div>
    </div>
    <div class="row">
        <div class="col-md-6" style="margin-bottom:10px;">
            <a href="/post/add" type="button" class="btn btn-default">New Post</a>
        </div>
    </div>
    <div class="col-md-6">
    <% postList.forEach(post =>
    { %>
        <div class="row">
            <a href="/post/1"><h4><%= post.title %></h4></a>
            <h5><%= post.author %></h5>
            <p>
                <a href="#" class="btn btn-default"role="button">
                    Edit
                </a>
            </p>
        </div>
    <% }) %>
    </div>
<%- include ("../_partial/_footer.ejs"); %>